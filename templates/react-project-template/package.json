{
  "name": "monitor-front",
  "version": "0.0.1",
  "description": "chrome插件",
  "scripts": {
    "dev": "ols dev",
    "build": "ols build",
    "commit": "git-cz",
    "lint:script": "eslint --ext .tsx,.ts,.js,.jsx --fix ./src",
    "lint:style": "stylelint --fix 'src/**/*.{less,css}' --syntax less"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,yml}": "prettier --write",
    "*.{less,css}": [
      "yarn lint:style"
    ],
    "*.{tsx,ts,js,jsx}": [
      "yarn lint:script"
    ]
  },
  "sideEffects": [
    "dist/*",
    "*.less",
    "*.css"
  ],
  "author": "xuzhudong",
  "license": "ISC",
  "resolutions": {
    "@types/react": "^18.0.9"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@formily/antd": "^1.3.8",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.0.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "lodash": "^4.17.20",
    "playwright-extra": "^4.3.6",
    "query-string": "^6.13.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.5.0",
    "react-router-dom": "^6.5.0"
  },
  "devDependencies": {
    "@ols-scripts/cli": "^0.0.2",
    "@ols-scripts/eslint-config": "^0.0.1",
    "@types/node": "^13.7.4",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.1",
    "typescript": "~4.1.0"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ]
}
